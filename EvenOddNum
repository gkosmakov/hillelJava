import java.util.Scanner;

public class EvenOddNum {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        int i1 = sc.nextInt();

        if (i1%2 == 0) {
            System.out.println("This number is even.");
        } else if (i1%2 != 0) {
            System.out.println("This number is odd.");
        }
    }
}
